ENV=development

# Session Encryption
# Generate with: openssl rand -base64 32
# Must be at least 32 characters
NUXT_SESSION_PASSWORD=your_generated_32_character_secret_here

# Database (optional - only if using Prisma or any database)
# Example: postgresql://postgres:postgres@localhost:5432/app_dev
NUXT_DATABASE_URL=

# Discord OAuth
# Create an app at https://discord.com/developers/applications
NUXT_OAUTH_DISCORD_CLIENT_ID=your_discord_client_id
NUXT_OAUTH_DISCORD_CLIENT_SECRET=your_discord_client_secret

# Google OAuth
# Create credentials at https://console.cloud.google.com
NUXT_OAUTH_GOOGLE_CLIENT_ID=your_google_client_id
NUXT_OAUTH_GOOGLE_CLIENT_SECRET=your_google_client_secret

# Add additional environment variables below as needed
NUXT_NODEMAILER_HOST=smtp.mailtrap.io
NUXT_NODEMAILER_PORT=2525
NUXT_NODEMAILER_AUTH_USER=user
NUXT_NODEMAILER_AUTH_PASS=pass
NUXT_NODEMAILER_FROM="admin@example.com"
NUXT_NODEMAILER_FROM_NAME="example"